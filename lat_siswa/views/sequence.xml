<odoo>
    <data noupdate="1">
        <record id="seq_register_number" model="ir.sequence">
            <field name="name">Register Number</field>
            <field name="code">register.number</field>
            <field name="prefix">REG.%(y)s.%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_receipt_number" model="ir.sequence">
            <field name="name">Receipt Number</field>
            <field name="code">receipt.number</field>
            <field name="prefix">REC.%(y)s.%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_student_number" model="ir.sequence">
            <field name="name">Student Number</field>
            <field name="code">student.number</field>
            <field name="prefix">STU.%(y)s.%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>